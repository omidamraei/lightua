{
  "name": "lightua",
  "version": "0.0.0-development",
  "description": "Lightweight, fast user-agent parser (TypeScript, zero deps).",
  "license": "MIT",
  "author": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omidamraei/lightua"
  },
  "bugs": {
    "url": "https://github.com/omidamraei/lightua/issues"
  },
  "homepage": "https://github.com/omidamraei/lightua#readme",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "prepublish": "npm run clean && npm run build",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.0.0",
    "typescript": "^5.6.2",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=16.20"
  },
  "keywords": [
    "user-agent",
    "ua",
    "parser",
    "browser-detection",
    "typescript",
    "zero-deps",
    "lightweight"
  ]
}
